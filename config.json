    "min_seeders": 0,

    "max_pages": 10
  },
  "output_settings": {
    "json_file": "output/torrents_{timestamp}.json",
    "csv_file": "output/torrents_{timestamp}.csv",
    "sqlite_file": "output/torrents_{timestamp}.db"
  }
}